<p id="notice"><%= notice %></p>

<h1>Listing Histogram Reference Diagnostics</h1>

<table>
  <thead>
    <tr>
      <th>Histogram</th>
      <th>Reference</th>
      <th>Diagnostic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @histogram_reference_diagnostics.each do |histogram_reference_diagnostic| %>
      <tr>
        <td><%= histogram_reference_diagnostic.histogram %></td>
        <td><%= histogram_reference_diagnostic.reference %></td>
        <td><%= histogram_reference_diagnostic.diagnostic %></td>
        <td><%= link_to 'Show', histogram_reference_diagnostic %></td>
        <td><%= link_to 'Edit', edit_histogram_reference_diagnostic_path(histogram_reference_diagnostic) %></td>
        <td><%= link_to 'Destroy', histogram_reference_diagnostic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Histogram reference diagnostic', new_histogram_reference_diagnostic_path %>
