<p id="notice"><%= notice %></p>

<h1>Listing Usabilities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usabilities.each do |usability| %>
      <tr>
        <td><%= usability.name %></td>
        <td><%= link_to 'Show', usability %></td>
        <td><%= link_to 'Edit', edit_usability_path(usability) %></td>
        <td><%= link_to 'Destroy', usability, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usability', new_usability_path %>
